    <li data-action="change-html">{{i18n "Change HTML"}}</li>
    {{else}}
    {{#unless this.isVoid}}
    <li data-action="change-text">{{i18n "Change Text"}}</li>
    {{/unless}}
    {{/if}}

    <div class="divider"></div>
    <li data-action="select-parent">{{i18n "Expand Selection"}}</li>
    </ul></script>
<script type="text/template" class="template" id="ELM.Activities.Templates.OneToOneExperienceEditor.ActionMenu.Location"><ul class="list">
    <li data-action="rename-loc">{{i18n "Rename Location"}}</li>
    <li data-action="delete-loc">{{i18n "Delete Location"}}</li>

    {{#with location}}
    {{#gt offers.length 2}}
    {{#each offers}}
    {{#equals ../../../selected @index}}
    {{!-- cannot delete the default offer --}}
    {{#notEquals @index 0}}
    <li data-action="remove-offer" data-id="{{@index}}">{{i18n "Remove"}}&nbsp;&quot;{{[jcr:title]}}&quot;</li>
    {{/notEquals}}
    {{/equals}}
    {{/each}}
    {{/gt}}
    {{/with}}

    <li data-action="edit-content">{{i18n "Edit Content..."}}</li>

    <div class="divider"></div>

    {{#with location}}
    {{#each_reverse offers}}
    {{#checkOfferToShow this}}
    <li data-action="change-inner-{{../../type}}" data-id="{{@reverseIndex}}">
        <i class="icon-check white {{#notEquals ../../../selected @reverseIndex}}visibility-hidden{{/notEquals}}"></i>
        <span>{{[jcr:title]}}</span>
    </li>
    {{/checkOfferToShow}}
    {{/each_reverse}}
    {{/with}}
    </ul>
</script>
<script type="text/template" class="template" id="ELM.Activities.Templates.MixContentWarningFirefox"><span class="message">{{i18n "To create experiences, your browser must be enabled to allow mixed content."}}</span><br />
    <span class="message"> {{i18n "Visitors to your site will NOT need to do this in order to view them"}}</span>
    <h3 class="bottom-border">{{i18n "Instructions"}}</h3>
    <div class="step row bottom-border">
        <div class="cell middle stepName">{{i18n "Step 1"}}.</div>
        <div class="cell middle stepDetail">{{i18n "Click the shield located on the left edge of your browser's address bar"}}</div>
    </div>
    <div class="step row bottom-border">
        <div class="cell middle stepName">{{i18n "Step 2"}}.</div>
        <div class="cell middle stepDetail">{{i18n "Click the down arrow on the &quot;Keep Blocking&quot; button, NOT the button itself" html=true}}</div>
    </div>
    <div class="step row">
        <div class="cell middle stepName">{{i18n "Step 3"}}.</div>
        <div class="cell middle stepDetail">{{i18n "Click &quot;Disable Protection on This Page&quot;" html=true}}</div>
    </div>
    <div class="step-image row">
        <div class="cell middle stepDetail">
            <img src="/apps/mac/target/components/common/images/firefox23_block_1.png" />
        </div>
    </div>
    <div class="step row lastStep bottom-border">
        <div class="cell middle stepName">{{i18n "Step 4"}}.</div>
        <div class="cell middle stepDetail">{{i18n "Click on &quot;Leave page&quot; in confirmation dialog" html=true}}</div>
    </div>
    <div class="row">
        <div class="cell middle helpicon">
            <i class="icon-help-circle"></i>
        </div>
        <div class="cell middle">
            <span>{{i18n "For instructions on how to <strong class='bold'>permanently</strong> enable this browser setting,<br />read our <a target='_blank' href='{0}'> Help Documentation </a> regarding browser settings." "http://microsite.omniture.com/t2/help/en_US/target/#target-Enabling_Mixed_Content_in_Firefox" html=true}}</span>
        </div>
    </div>
</script>
