                {{else}}
                {{#if ../../../reportSuite}}
                {{number conversions 2}}{{#equals metricFormat 'percent'}}%{{/equals}}
                {{else}}
                {{#number}}{{getValueFromObj this ../../../../../reportFilter.successCriteriaMetric}}{{/number}}
