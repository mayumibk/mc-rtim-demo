<script type="text/template" class="template" id="ELM.Activities.Templates.AlgorithmEditor.BasedOnItem" data-partial="BasedOnItem">{{#notEquals configuration.type "fullDataOptimizing"}}
    {{> PriceOption this}}
    {{else}}
    <div class="align-l margin-b-0-25">{{i18n "What should the connection strength be between the recommended products and the key?"}}</div>
    <div class="margin-b-1 height-2-375">
        <div class="float-l">
            <div class="product-confidence display-inline-block">
                <div class="slider ticked filled" data-init="slider">
                    <label>
                        <input type="range" value="{{getValueForProductConfidence details.productConfidence}}" min="0" max="4" step="1" id="productConfidence">
                    </label>
                </div>
                <div class="tooltip info arrow-left margin-b-0-25 margin-t-0-25 display-none"></div>
            </div>
        </div>
    </div>
    {{/notEquals}}
    <div class="align-l margin-b-0-25">
        {{i18n "Do you want to limit products by inventory?"}}&nbsp;{{i18n "If so, how few products can be available before the product will stop showing?"}}
    </div>
    <div class="margin-b-1 height-2-375">
        <div class="float-l padding-t-0-5 padding-l-1">
            <label class="lh-1"><input type="checkbox" class="inventory-input width-0-imp" {{#if details.minInventory}}checked="true"{{/if}}><span></span></input>
        </div>
        <div class="float-l padding-l-0-75">
            <div class="numberinput min-inventory width-11" data-init="numberinput" data-min="0" {{#if details.minInventory}}{{else}}data-disabled="true"{{/if}}>
            <button type="button" class="decrement">decrement</button>
            <input type="text" value="{{details.minInventory}}">
            <button type="button" class="increment">increment</button>
        </div>
    </div>
    </div>
    {{#notEquals configuration.type "fullDataOptimizing"}}
    <div class="align-l margin-b-0-25">{{i18n "What other rules should the recommendation obey?"}}</div>
    {{#each details.inclusionrules}}
    {{#gt @index 0}}
    <div class="margin-b-0-25">{{i18n "AND"}}</div>
    {{/gt}}
    <div class="inclusion-rule margin-b-0-5 height-2-375" data-index="{{@index}}">
      <span class="select width-11 attribute">
        <button type="button" class="ellipsis-text">{{[jcr:title]}}</button>
        <select>
            <option data-type="entity" value="REFRESH_ATTRIBUTES" class="font-italic">*** {{i18n "Refresh List"}} ***</option>
            {{#each ../entityAttributes}}
            <option {{#equals ../attribute [jcr:title]}}selected="selected"{{/equals}} value="{{[jcr:title]}}">{{[jcr:title]}}</option>
            {{/each}}
        </select>
      </span>
      <span class="select width-11 evaluator margin-l-0-75">
        <button type="button">{{i18n "Choose Evaluator"}}</button>
        <select>
            <option {{#equals operation ""}}selected="selected"{{/equals}} value="">{{i18n "Choose Evaluator"}}</option>
            <option {{#equals operation "dynamicallyMatches"}}selected="selected"{{/equals}} value="dynamicallyMatches">{{i18n "matches"}}</option>
            <option {{#equals operation "dynamicallyDoesNotMatch"}}selected="selected"{{/equals}} value="dynamicallyDoesNotMatch">{{i18n "does not match"}}</option>
            {{#notEquals attribute "value"}}
            <option {{#equals operation "dynamicallyRanges"}}selected="selected"{{/equals}} value="dynamicallyRanges">{{i18n "is between"}}</option>
            {{/notEquals}}
            {{#in attribute "environment" "inventory" "margin" "value"}}
            {{> InclusionRuleNumericOperators this}}
            {{else}}
            {{> InclusionRuleNonNumericOperators this}}
            {{/in}}
        </select>
      </span>
        {{#in operation "dynamicallyMatches" "dynamicallyDoesNotMatch"}}
        <div class="display-inline-block padding-t-0-5 padding-l-0-75 max-width-11">{{getInclusionRuleOperand ../../configuration.key attribute}}</div>
        {{else}}
        {{#equals operation "dynamicallyRanges"}}
        <input type="text" class="min-width-4-imp width-4-imp low-range margin-l-1-imp" placeholder="%" value="{{lowRange}}" pattern="\d*" title="{{i18n "Enter positive integer"}}" />
        <div class="padding-tb-0-5-rl-0-25 display-inline-block">{{i18n "and"}}</div>
        <input type="text" class="min-width-4-imp width-4-imp high-range" placeholder="%" value="{{highRange}}" pattern="\d*"  title="{{i18n "Enter positive integer"}}" />
        {{else}}
        <input type="text" class="margin-l-0-75-imp values" value="{{#each values}}{{#gt @index 0}}, {{/gt}}{{this}}{{/each}}" />
        {{/equals}}
        {{/in}}
        <div class="padding-l-0-75 display-inline-block">
            <button class="icon-delete small margin-l-0-imp delete-inclusion-rule" title="{{i18n "Delete"}}"></button>
        </div>
    </div>
    {{/each}}
    {{/notEquals}}</script>
