<script type="text/template" class="template" id="ELM.Activities.Templates.AlgorithmEditor.PriceOption" data-partial="PriceOption"><div class="align-l margin-b-0-25">{{i18n "What price rules should the products from this recommendation obey?"}}</div>
    <div class="margin-b-1 height-2-375 price-option">
        <div class="float-l width-14">
    <span class="select width-14" id="price">
      <button type="button" class="ellipsis-text">{{i18n "Choose price rule"}}</button>
      <select>
          <option value="">{{i18n "Choose price rule"}}</option>
          {{#in configuration.key "currentItem" "customProfileAttribute"}}
