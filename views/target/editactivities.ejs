<script type="text/template" class="template" id="ELM.Activities.Templates.ActivityDetails"><div class="detailsHeader"></div>

    <!-- Placeholder for sync error notification -->
    <div class="error alert large syncErrorNotifier {{#activitySyncFailed sync.root}}{{else}}hidden{{/activitySyncFailed}}">
        <button class="close">x</button>
        <strong>{{i18n "Sync error details"}}</strong>
        <div class="syncFailureReason">{{callToAction}} &nbsp;</div>
    </div>

    <div class="row fixed activityStatusRow bg-e7e7e7">
        <div class="cell middle status container-relative {{#activitySyncFailed sync.root}}inactive{{/activitySyncFailed}}">
            {{#activitySyncFailed sync.root}}<i class="icon-alert small red syncErrorIcon"></i>{{/activitySyncFailed}}
            <span class="activityStatus">{{readGlobalVar "ELM.Activities.displayStatus" status}}</span>
            {{#activitySyncFailed sync.root}}<span>({{i18n "sync error"}})</span>{{/activitySyncFailed}}
            {{#isActivityUpdating status sync.root.targetSyncPending}}<span class="wait"></span>{{/isActivityUpdating}}
            {{#equals status "LIVE"}}&nbsp;({{getDate activatedDate format="MMM DD YYYY" comment="Date format pattern for moment.js. e.g. Nov 13 2013 (http://momentjs.com/docs)"}}&nbsp;-&nbsp;{{i18n "Present"}}){{/equals}}
        </div>

        <div class="cell middle winner-experience">
            {{#each experiences}}
            {{#if winner}}
            <i class="icon-ribbon small"></i>
            &nbsp;{{i18n "Winning Experience"}}:&nbsp;{{[jcr:title]}}
            {{/if}}
            {{/each}}
        </div>

        {{#ifGlobalVar "ELM.isProjectedEarningsEnabled"}}
        <div class="cell middle estimated-lift">
            {{#if bestOpportunity}}
            <i class="icon-graphtrend small"></i>
        <span class="morelink">
          &nbsp;{{i18n "Predicted Revenue for Winner"}}:&nbsp;{{formatEstimatedValue bestOpportunity 1}}
          <span class="tooltip arrow-left">
            {{i18n "Predicted increase in revenue if 100% of your audience sees the winning experience."}}&nbsp;
            <a href="http://microsite.omniture.com/t2/help/en_US/target/#target-Estimating_Lift_in_Revenue" target="_blank">{{i18n "Learn more"}}</a>
          </span>
        </span>
            {{/if}}
        </div>
        {{/ifGlobalVar}}

        <div class="cell right v-align-middle padding-r-1 width-15">
            <span class="font-bold">{{i18n "Type"}}:&nbsp;</span>
            <span>{{getActivityDisplayType activityType}}</span>
        </div>
    </div>

    <div class="tabs nav padding-t-0-5 bg-e7e7e7" data-init="tabs">
        <nav>
            <a href="#activitydetails/{{activityType}}/:{{id}}" data-toggle="tab" class="tab-header active">{{i18n "Overview"}}</a>
            {{#equals activityType "MVT"}}
            <a href="#reports-contribution/{{activityType}}/:{{id}}" data-toggle="tab" class="tab-header reportsTabHeader" data-reportId="locationContributionReport">{{i18n "Location Contribution"}}</a>
            <a href="#reports/{{activityType}}/:{{id}}" data-toggle="tab" class="tab-header reportsTabHeader" data-reportId="activityReport">{{i18n "Experience Performance"}}</a>
            {{else}}
            <a href="#reports/{{activityType}}/:{{id}}" data-toggle="tab" class="tab-header reportsTabHeader">{{i18n "Reports"}}</a>
            {{/equals}}
            {{#ifGlobalVar "ELM.isActivityCollisionEnabled"}}
            <a href="#collision/{{activityType}}/:{{id}}" data-toggle="tab" class="tab-header collision-tab-header cursor-pointer">{{i18n "Collisions"}}</a>
            {{/ifGlobalVar}}
        </nav>

        <section class="active">
            <div class="activityOverview">{{i18n "Overview of Activity"}}</div>
        </section>
        {{#equals activityType "MVT"}}
        <section>
            <div class="activity-contribution-report">
                <div class="alert info center hidden">
                    <strong>{{i18n "Info"}}</strong>
                    <div>{{i18n "There's no report data available."}}</div>
                </div>
            </div>
        </section>
        {{/equals}}
        <section>
            <div class="activityReports">
                <div class="alert info center hidden">
                    <strong>{{i18n "Info"}}</strong>
                    <div>{{i18n "There's no report data available."}}</div>
                </div>
            </div>
        </section>
        {{#ifGlobalVar "ELM.isActivityCollisionEnabled"}}
        <section>
            <div class="activityCollisions collision">
                <div class="row">
                    <div class="cell wait-body retrieving">
                        <div class="wait large"></div>
                        <div class="waitMessage">{{i18n "No data."}}</div>
                    </div>
                </div>
            </div>
        </section>
        {{/ifGlobalVar}}
    </div>
</script>
<script type="text/template" class="template" id="ELM.Activities.Templates.ActivityDetailsHeader"><div class="toolbar">
    <div class="icongroup left">
        <button class="icon-back" title="{{i18n "Go back"}}" data-trigger-event="backPressed"></button>
    </div>

    <div class="left">
        <h1>{{[jcr:title]}}</h1>
    </div>

    <div class="right lh-2-375 padding-r-1">
        {{#in status "INACTIVE" "DRAFT"}}
        <button class="primary headerBtn activateButton activity-action-button" {{#equals status "DRAFT"}}disabled="disabled"{{else}}{{#unless isUserApprover}}disabled="enabled"{{/unless}}{{/equals}} data-action-id="activate">
        <i class="icon-play-circle btnIcon small"></i>{{i18n "Activate"}}
        </button>
        {{/in}}
        {{#in status "LIVE" "SCHEDULED" "ENDED" "DEACTIVATING"}}
        <button class="primary headerBtn deactivateButton activity-action-button" {{#equals status "DEACTIVATING"}}disabled="disabled"{{else}}{{#unless isUserApprover}}disabled="disabled"{{/unless}}{{/equals}} data-action-id="deactivate">
        <i class="icon-pause-circle btnIcon small"></i>
        {{i18n "Deactivate"}}
        </button>
        {{/in}}
        <a href="#edit/{{activityType}}/:{{id}}" class="icon-edit font-0-9375 display-inline-block-i link-no-underline padding-l-1 withLabel">{{i18n "Edit Activity"}}</a>
        {{!-- <button class="icon-share" title="{{i18n "Share a card"}}"></button> --}}
        {{#in activityType "A-B" "Experience-Targeting" "recommendation"}}
        <button class="icon-pinoff font-size-0-9375-imp display-inline-block-i padding-l-1 withLabel" data-trigger-event="cardShared" {{#in status "DRAFT"}}disabled="disabled"{{/in}}>{{i18n "Pin to Board"}}</button>
        {{/in}}
        {{#notEquals activityType '1-1'}}
        {{#in status "INACTIVE" "LIVE" "SCHEDULED"}}
        <a href="#view-experience-dialog" class="icon-link font-0-9375 display-inline-block-i link-no-underline padding-l-1 withLabel" data-toggle="modal" data-type="default">{{i18n "View Experience URLs"}}</a>
        {{#equals activityType "recommendation"}}
        <a href="" class="icon-download download-data-link font-0-9375 display-inline-block-i link-no-underline padding-l-1 withLabel" download>{{i18n "Download data"}}</a>
        {{/equals}}
        {{/in}}
        {{/notEquals}}
    </div>
    </div>
</script>
