<script type="text/template" class="template" id="ELM.Activities.Templates.TemplateTestingCriteria">{{#each criteria}}
    <div class="rule margin-b-1 {{#if incomplete}}incomplete{{/if}}">
        <label class="and-label margin-r-0-2">{{i18n "AND"}}</label>
        <div class="nativedropdown">
            <select class="step"  data-index="{{@index}}">
                <option value="" data-modelid="" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter ""}}selected="selected"{{/equals}}>{{i18n "Add Criteria"}}</option>
                <option value="currentPage.url" data-modelid="_url" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter "currentPage.url"}}selected="selected"{{/equals}}>{{i18n "URL"}}</option>
                <option value="currentPage.domain" data-modelid="_domain" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter "currentPage.domain"}}selected="selected"{{/equals}}>{{i18n "Domain"}}</option>
                <option value="currentPage.path" data-modelid="_path" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter "currentPage.path"}}selected="selected"{{/equals}}>{{i18n "Path"}}</option>
                <option value="currentPage.query" data-modelid="_query" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter "currentPage.query"}}selected="selected"{{/equals}}>{{i18n "Query"}}</option>
                <option value="mbox" data-type="MBOX" data-modelid="_mboxparameter" {{#equals type "MBOX"}}selected="selected"{{/equals}}>{{i18n "mbox parameter"}}</option>
            </select>
        </div>
        {{#equals type "MBOX"}}
        <div class="nativedropdown">
            <select class="input-mbox step"  data-index="{{@index}}">
                <option value="" selected="selected">{{i18n "mbox name"}}</option>
                {{#if ../../mboxes}}
                {{#each ../../../mboxes}}
                <option value="{{id}}" data-key="mbox" {{#equals id ../mbox}}selected="selected"{{/equals}}>{{[jcr:title]}}</option>
                {{/each}}
                {{/if}}
            </select>
        </div>
        {{#if mbox}}
        <div class="nativedropdown">
            <select class="input-parameter step" data-index="{{@index}}">
                <option value="" selected="selected">{{i18n "Parameter"}}</option>
                {{#if ../parameters}}
                {{#each ../parameters}}
                <option value="{{name}}" data-key="parameter" {{#equals name ../parameter}}selected="selected"{{/equals}}>{{name}}</option>
                {{/each}}
                {{/if}}
            </select>
        </div>
        {{#if parameter}}
        {{> OperatorAndOperand this}}
        {{/if}}
        {{/if}}
        {{/equals}}
        {{#equals parameter "currentPage.query"}}
        <input type="text" class="step width-12-5 margin-tb-0-125-rl-0-imp" data-key="subParameter" placeholder="{{i18n "Query Parameter"}}" value="{{subParameter}}" data-order="3" data-index="{{@index}}">
        {{#if subParameter}}
        {{> OperatorAndOperand this}}
        {{/if}}
        {{/equals}}
        {{#in parameter "currentPage.url" "currentPage.domain" "currentPage.path"}}
        {{#if parameter}}
        {{> OperatorAndOperand this}}
        {{/if}}
        {{/in}}
        <button class="icon-delete right padding-r-2" data-index="{{@index}}"></button>
    </div>
    {{/each}}
    <div class="default nativedropdown">
        <select class="criteriaSelector">
            <option value="" data-modelid="" data-type="" data-key="" {{#equals parameter ""}}selected="selected"{{/equals}}>{{i18n "Add Criteria"}}</option>
            <option value="currentPage.url" data-modelid="_url" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter "currentPage.url"}}selected="selected"{{/equals}}>{{i18n "URL"}}</option>
            <option value="currentPage.domain" data-modelid="_domain" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter "currentPage.domain"}}selected="selected"{{/equals}}>{{i18n "Domain"}}</option>
            <option value="currentPage.path" data-modelid="_path" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter "currentPage.path"}}selected="selected"{{/equals}}>{{i18n "Path"}}</option>
            <option value="currentPage.query" data-modelid="_query" data-type="SITE_PAGE" data-key="parameter" {{#equals parameter "currentPage.query"}}selected="selected"{{/equals}}>{{i18n "Query"}}</option>
            <option value="mbox" data-type="MBOX" data-modelid="_mboxparameter" {{#equals type "MBOX"}}selected="selected"{{/equals}}>{{i18n "mbox parameter"}}</option>
        </select>
    </div></script>
<script type="text/template" class="template" id="ELM.Activities.Templates.TemplateTestingCriteria.OperatorAndOperand" data-partial="OperatorAndOperand"><div class="nativedropdown">
    <select class="step" data-index="{{@index}}">
        <option value="" data-key="operator" {{#equals operator ""}}selected="selected"{{/equals}}>{{i18n "Choose evaluator"}}</option>
        <option value="containsIgnoreCase"  data-key="operator"  {{#equals operator "containsIgnoreCase"}}selected="selected"{{/equals}}>{{i18n "contains"}}</option>
        <option value="doesNotContainIgnoreCase" data-key="operator" {{#equals operator "doesNotContainIgnoreCase"}}selected="selected"{{/equals}}>{{i18n "does not contain"}}</option>
        <option value="equals" data-key="operator"  {{#equals operator "equals"}}selected="selected"{{/equals}}>{{i18n "is (case sensitive)"}}</option>
        <option value="doesNotEqualIgnoreCase" data-key="operator"  {{#equals operator "doesNotEqualIgnoreCase"}}selected="selected"{{/equals}}>{{i18n "is not"}}</option>
        <option value="startsWithIgnoreCase" data-key="operator"  {{#equals operator "startsWithIgnoreCase"}}selected="selected"{{/equals}}>{{i18n "Starts with"}}</option>
        <option value="endsWithIgnoreCase" data-key="operator"  {{#equals operator "endsWithIgnoreCase"}}selected="selected"{{/equals}}>{{i18n "Ends with"}}</option>
    </select>
    </div>
    {{#if operator}}
    <textarea cols="27" rows="2" class="input-operands textarea-step margin-tb-0-125-rl-0-imp"  data-key="operands" data-index="{{@index}}">{{join operands "\n"}}</textarea>
    {{/if}}</script>
<script type="text/template" class="template" id="ELM.Activities.Templates.TemplateTestingCriteria.LegacyTargetRulesTemplate">{{#each rulesString}}
    <div class="target-rule margin-b-1">
        <label class="or-label margin-r-0-2 lh-1">{{i18n "OR"}}</label><label class="lh-1">{{this}}</label>
    </div>
    {{/each}}</script>
<script type="text/template" class="template" id="ELM.Activities.Templates.TargetTemplateTestingSummary" data-partial="TargetTemplateTestingSummary"><section class="width-100p margin-l-0 {{#equals activityType "recommendation"}}margin-t-1-7{{else}}margin-t-0{{/equals}}">
    {{#if targets.template.targets}}
