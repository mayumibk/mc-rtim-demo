<script type="text/template" class="template" id="ELM.Activities.Templates.Summary"><div id="summaryTop" class="alert info row" style="display: none;">
    <div class="cell">
        <h2>{{i18n "Status: Requires approval from Kevin Michael."}}</h2>
    </div>
    <div class="cell middle right approveButtons">
        <a class="button primary" href="#">{{i18n "Submit for Approval"}}</a>
        <a class="button" href="#">{{i18n "Save &amp; Close"}}</a>
    </div>
    </div>

    <h1 class="activityName">
        {{#if [jcr:title]}}
        {{[jcr:title]}}
        {{else}}
        {{i18n "Untitled Activity"}}
        {{/if}}
        &nbsp;<sup class="badge">{{activityType}}</sup>
    </h1>

    {{#equals activityType "Experience-Targeting"}}
    {{>ExperienceTargetingInfographic this}}
    {{else}}
    {{! Note: The only difference between the markup here and in the Edit Activity is the input tag for experience splits; make any markup fixes in both locations.
    TO-DO: This should be removed to a partial. }}
    <section class="flowDiagram">
        <div class="diagRow titleRow">
            <div class="column visitorsCol"><h3>{{i18n "Audience"}}</h3></div>
            <div class="column audiencesCol"><h3>{{i18n "Audiences"}}</h3></div>
            <div class="column experiencesCol"><h3>{{i18n "Experiences"}}</h3></div>
        </div>
        <div class="audienceExp">

            <div class="diagRow">
                <div class="column visitorsCol">
                    <div class="audienceContainer">
                        <div class="leftLinesCol">
                            <div class="lineCell">&nbsp;</div>
                            <div class="lineCell">&nbsp;</div>
                        </div>
                        <div class="diagBox visitorsBox">&nbsp;</div>
                        <div class="rightLinesCol">
                            <div class="lineCell lineBottom">&nbsp;</div>
                            <div class="lineCell lineTop">&nbsp;</div>
                        </div>
                    </div>
                </div> <!-- visitorsCol -->

                <div class="column audiencesCol">
                    <div class="audienceContainer">
                        <div class="leftLinesCol">
                            <div class="lineCell lineBottom">&nbsp;</div>
                            <div class="lineCell lineTop">&nbsp;</div>
                        </div>

                        <div class="diagBox audiencePlaceHolder audienceBox">
                            {{#if audience}}
                            {{> ActivityAudience audience}}
                            {{else}}
                            {{> ActivityNoAudience}}
                            {{/if}}
                        </div>

                        <div class="rightLinesCol">
                            <div class="lineCell lineBottom">&nbsp;</div>
                            <div class="lineCell lineTop">&nbsp;</div>
                        </div>
                    </div>
                </div> <!-- audiencesCol -->

                {{#if experiences}}
                {{#each experiences}}
                <div class="column experiencesCol displayOnly">
                    <div class="experienceContainer" data-experience="{{[jcr:title]}}">
                        <div class="leftLinesCol">
                            <div class="lineCell lineBottom lineLeft">&nbsp;</div>
                            <div class="lineCell lineTop lineLeft">
                                <input type="text" class="expSplit" value="{{trafficSplit}}%" disabled="disabled" />
                            </div>
                        </div>
                        <div class="diagBox experienceBox noLineTop">
                            {{> ActivityExperience this}}
                        </div>
                    </div>
                </div> <!-- experiencesCol -->
            </div> <!-- diagRow -->

            <div class="diagRow">
                <div class="column visitorsCol">
                </div> <!-- visitorsCol -->

                <div class="column audiencesCol">
                    <div class="audienceContainer">
                        <div class="leftLinesCol">
                            <div class="lineCell">&nbsp;</div>
                            <div class="lineCell">&nbsp;</div>
                        </div>
                        <div></div>
                    </div>
                </div> <!-- audiencesCol -->
                {{/each}}
                <div class="column experiencesCol">
                    <div class="experienceContainer">
                        <div class="leftLinesCol">
                            <div class="lineCell">&nbsp;</div>
                            <div class="lineCell">&nbsp;</div>
                        </div>
                        <div class="diagBox">
                            {{! <span class="addExpLabel">Add Experience</span> }}
                        </div>
                    </div>
                </div> <!-- experiencesCol -->
                {{else}}
                <div class="column experiencesCol">
                    <div class="experienceContainer">
                        <div class="leftLinesCol">
                            <div class="lineCell lineBottom">&nbsp;</div>
                            <div class="lineCell lineTop">
                                <div class="cell middle"></div>
                            </div>
                        </div>
                        <div class="diagBox experienceBox">
                            {{i18n "None added"}}
                        </div>
                    </div>
                </div> <!-- experiencesCol -->
                {{/if}}

            </div>

        </div> <!-- audienceExp -->
    </section>
    {{/equals}}
</script>
<script type="text/template" class="template" id="ELM.Activities.Templates.DateWidget" data-partial="ActivityDateSelectorWidget"><div class="dateOptionDropdown v-align-top"></div>
    <div class="largeDatepicker v-align-top">
        <div class="datepicker dateWidget" data-min-date="today" data-stored-format="MMM DD, YYYY" data-displayed-format="{{i18n "MMM DD, YYYY" comment="Date format pattern for moment.js. e.g. Nov 13, 2013 (http://momentjs.com/docs)"}}">
        <button class="icon-calendar small"></button>
        <input type="date" value="{{getDate this}}" readonly="readonly" />
    </div>
    <div class="datepicker timeWidget" data-stored-format="HH:mm" data-displayed-format="{{i18n "HH:mm" comment="Time format pattern for moment.js. e.g. 12:48 (http://momentjs.com/docs)"}}">
    <button class="icon-calendar icon-clock small"></button>
    <input type="time" value="{{getTime this}}" readonly="readonly" />
    </div>
    </div>
</script>
<script type="text/template" class="template" id="ELM.Activities.Templates.DateOptions"><span class="select">
    <button></button>
    <select>
        {{#each options}}
        <option value="{{value}}" {{#if value}}selected="selected"{{/if}}>{{label}}</option>
        {{/each}}
    </select>
    </span></script>

<script type="text/template" class="template" id="ELM.Activities.Templates.Overview">{{#with targets.template}}
    {{#if activityUrl}}
    <div class="overview-section border-bottom">
        <h4>{{i18n "Activity Location"}}</h4>
        <div class="target-criteria">
            <div>{{activityUrl}}</div>
            {{#if legacy}}
            {{#if templateString}}
            <span>{{i18n "Activity URL(s)"}}:&nbsp;{{i18n "Include any page where"}}&nbsp;</span>
            {{#eachString templateString "OR"}}
            <div class="{{#equals @index 0}}display-inline-block{{/equals}}">
                {{#notEquals @index 0}}
                <span class="color-969696">{{i18n "OR"}}&nbsp;</span>
                {{/notEquals}}
                <span>{{this}}</span>
            </div>
            {{/eachString}}
            {{/if}}
            {{else}}
            {{#if targets}}
            {{#with targets.[0]}}
            <span>{{i18n "Activity URL(s)"}}:&nbsp;{{i18n "Include any page where"}}&nbsp;</span>
            {{#if children}}
            {{#each children}}
            <div class="{{#equals @index 0}}display-inline-block{{/equals}}{{#gt @index 4}}more-criteria display-none{{/gt}}">
                {{#notEquals @index 0}}
                <span class="color-969696">{{toUpperCase ../../operator}}&nbsp;</span>
                {{/notEquals}}
                <span>{{getTemplateTestingCriteriaParameter parameter type subParameter}}&nbsp;{{getTemplateTestingCriteriaOperator operator}}&nbsp;{{join operands ", "}}</span>
            </div>
            {{/each}}
            {{#gt children.length 5}}
            <div>
                <a class="target-criteria-view-toggle see-all link-no-underline">({{i18n "See all"}})</a>
                <a class="target-criteria-view-toggle see-less link-no-underline display-none">({{i18n "See less"}})</a>
            </div>
            {{/gt}}
            {{else}}
            <span>{{getTemplateTestingCriteriaParameter parameter type subParameter}}&nbsp;{{getTemplateTestingCriteriaOperator operator}}&nbsp;{{join operands ", "}}</span>
            {{/if}}
            {{/with}}
            {{/if}}
            {{/if}}
            <br/>
        </div>
        {{#notEquals ../../activityType '1-1'}}
        {{#in ../../../status "INACTIVE" "LIVE" "SCHEDULED"}}
        <div><a href="#view-experience-dialog" data-toggle="modal" data-type="default">{{i18n "View Experience URL(s)"}}</a></div>
        {{/in}}
        {{/notEquals}}
    </div>
    {{/if}}
    {{/with}}

    <div id="view-experience-dialog" class="modal experience-url-modal">
        <div class="modal-header">
            <h2>{{i18n "Experience URL(s)"}}</h2>
            <button type="button" class="close" data-dismiss="modal">x</button>
        </div>
        <div class="modal-body">
            <div class="modal-body-inner scrollable">
                <p>{{i18n "Please validate this Adobe Target Activity by visiting each of the experience URL(s) below:"}}</p>
                <h4>{{i18n "Activity name"}}:&nbsp;<span class="url-info-text">{{[jcr:title]}}</span></h4>
                <h4>{{i18n "Activity URL"}}:&nbsp;<span class="url-info-text">{{targets.template.activityUrl}}</span></h4>

                {{#equals activityType "Experience-Targeting"}}
                <strong>{{i18n "Audience"}}</strong>&nbsp;-&nbsp;<strong>{{i18n "Experiences"}}</strong>
                {{else}}
                <h4>{{i18n "Audience"}}:&nbsp;
                    {{#getValueFromObj [elm:references] ../targets/[ref:audiences]}}
                    <span class="url-info-text">{{[jcr:title]}}</span>
                    {{/getValueFromObj}}
                </h4>
                <h4>{{i18n "Experiences"}}:</h4>
                {{/equals}}
                <ul id="previewLinksList" class="preview-links"></ul>
            </div>
        </div>
        <div class="modal-footer">
            <button data-dismiss="modal">{{i18n "Cancel"}}</button>
        </div>
    </div>

    {{#if [jcr:description]}}
    <div class="overview-section border-bottom">
        <h4>{{i18n "Objective"}}</h4>
        <div>{{getStringWithLineBreaks [jcr:description]}}</div>
    </div>
    {{/if}}

    <div class="overview-section border-bottom">
        <h4>{{i18n "Goal"}}</h4>
        {{#with metrics.[0]}}
        <table class="goal">
            <tr>
                <td class="label"><span>{{i18n "I want to..."}}</span></td>
                <td>
                    {{#equals type "analytics"}}
                    {{i18n "Increase {0}" jcr:title}}
                    {{else}}
                    {{i18n "Increase {0}" type}}
                    {{/equals}}
                </td>
            </tr>
            {{#notEquals type "analytics"}}
            <tr>
                <td class="label">{{i18n "Measured by..."}}</td>
                <td>
                    {{#if action}}
                    {{#buildMetricsMeasurementDisplayRule action constraints}}{{/buildMetricsMeasurementDisplayRule}}
                    {{else}}
                    {{#equals type "engagement"}}
                    {{#equals engagementType "pageViews"}}{{i18n "Page Views"}}{{/equals}}
                    {{#equals engagementType "customScoring"}}{{i18n "Custom Scoring"}}{{/equals}}
                    {{#equals engagementType "timeOnSite"}}{{i18n "Time on Site"}}{{/equals}}
                    {{/equals}}
                    {{/if}}
                </td>
            </tr>
            {{/notEquals}}
        </table>
        {{/with}}
    </div>

    <div class="overview-section border-bottom">
        <h4>{{i18n "Info"}}</h4>
        <div>{{i18n "Priority"}}:&nbsp;{{getActivityPriority priority}}</div>
        {{#if notes}}
        <div>{{i18n "Note"}}:&nbsp;{{getStringWithLineBreaks notes}}</div>
        {{/if}}
    </div>

    <div class="overview-section">
        <h4>{{i18n "Target"}}</h4>
        <div class="overviewSummary"></div>
    </div>
</script>


<script type="text/template" class="template" id="ELM.Activity.Templates.Collision">{{i18n "The following activities are live or scheduled to be live on the same page(s) as this activity."}}&nbsp;{{i18n "To ensure the proper activity displays, check its priority and audience compared to other activities set for the same page."}}
    <ul class="activity-collision-list"></ul></script>
<script type="text/template" class="template" id="ELM.Activity.Templates.CollisionListItem"><div class="collapsible collision-list-row" data-init="collapsible">
    <h3>
        <em>{{i18n "Activity URL"}}:</em> &nbsp;<a href="{{url}}" target="_blank">{{url}}</a>
        <br/>
        <em>{{i18n "Mbox"}}:</em> &nbsp;{{mbox}}
    </h3>
    <ul class="list">
        <li class="header activity-row">
            <div class="row fixed">
