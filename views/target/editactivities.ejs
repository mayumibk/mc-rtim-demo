        <div class="align-l margin-b-0-25">{{i18n "How much data from the past should we base this recommendation on?"}}</div>
        <div class="days-count margin-b-0-5">
            <div class="slider ticked filled" data-init="slider">
                <label>
                    <input type="range" value="{{getValueForDaysCount details.daysCount}}" min="{{#equals configuration.type "fullDataOptimizing"}}0{{else}}1{{/equals}}" max="{{#equals configuration.type "fullDataOptimizing"}}3{{else}}5{{/equals}}" step="1" id="daysCount">
                </label>
            </div>
            <div class="tooltip info arrow-left margin-b-0-25 margin-t-0-25 display-none"></div>
        </div>
        {{#notIn configuration.key "currentCategory" "favoriteCategory"}}
        {{#equals reportSuites undefined}}
        <div class="display-inline-block margin-l-1 margin-t-05">
            <span class="wait v-align-middle"></span>
            <span class="margin-l-0-5">{{i18n "Please wait"}}</span>
        </div>
        {{/equals}}
        {{#equals configuration.key "popularity"}}
        {{> DataSourceBasedOnPopularity this}}
        {{else}}
        {{> DataSource this}}
        {{/equals}}
        {{/notIn}}
    </div>
    {{/notEquals}}</script>
