            <h2 class="margin-b-0 algorithm-picker-header">{{i18n "Select Criteria"}}</h2>
        </div>
        <div class="float-r padding-t-0-75 font-0-9375">
            {{i18n "You have selected <strong class='font-1-25 algorithm-selected-count'>{0}</strong> Criteria & <strong class='font-1-25 template-selected-count'>{1}</strong> Design(s)" selectedAlgorithms.length selectedTemplates.length html=true}}
