    <!-- Defining ELM namespace variables (Values come from global.jsp) -->
    <script type="text/javascript">
        var ELM = ELM || {};

        ELM.client= 'adobe-tech-marketing';
        ELM.baseURI = '/target';
        ELM.bullseyeFolder = 'target';
        ELM.bullseyeSelector = 'at';
        ELM.defaultSimulatorUrl = 'http://marketlng.adobe.com/target/wefinance/en/personal.html';
        ELM.isExpThumbsEnabled = true;
        ELM.isProjectedEarningsEnabled = true;
        ELM.isProxyEnabledInEE = true;
        ELM.defaultReportSuite = '';
        ELM.scEnabled = true;
        ELM.scCompany = 'Adobe Tech Marketing';
        ELM.reportingEngine = 'analytics';
        ELM.isBullseyePremium = true;
        ELM.customGlobalMbox = '';
        ELM.industryType = '';
        ELM.isRecsEnabled = true && true;
        ELM.tntHost = "http://marketlng.adobe.com";
        ELM.tntClientCode = "bullseyedemo";
        ELM.clientLocale = 'en_US';
        ELM.isMobileTargetingEnabled = true;
        ELM.isAlgorithmFilteringEnabled = true;

        if (ELM.defaultSimulatorUrl !== "") {
            if (!RegExp(/^[a-z]+:\/\//).test(ELM.defaultSimulatorUrl)) {
                ELM.defaultSimulatorUrl =  window.location.protocol + '//' + window.location.host + ELM.defaultSimulatorUrl;
            }
        }

        if (ELM.client !== '') {
            ELM.baseURI += '/' + encodeURIComponent(ELM.client) + '/' + ELM.bullseyeFolder;
        }
    </script>






