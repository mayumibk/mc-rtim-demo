                    <span class="label v-align-top"><label>{{i18n "Start"}}</label></span>
                    <input type="hidden" value="{{startDate}}" id="startDate" />
                    {{> ActivityDateSelectorWidget startDate}}
                </div>
                <div id="endDateSection" class="row fixed">
                    <span class="label v-align-top"><label>{{i18n "End"}}</label></span>
                    <input type="hidden" value="{{endDate}}" id="endDate" />
                    {{> ActivityDateSelectorWidget endDate}}
                </div>
            </div>
        </div>
    </section>

    <section class="width-40">
        <div class="padding-b-1">
            <h4 class="padding-b-0-25">{{i18n "Priority"}}</h4>
            <div class="well padding-t-0-5-imp padding-b-0-5-imp">
                <span>{{i18n "Low"}}</span>
                <div class="slider ticked filled prioritySlider">
                    <input id="priority" type="range" value="{{priority}}" min="0" max="2"/>
                </div>
                <span>{{i18n "High"}}</span>
            </div>
        </div>
