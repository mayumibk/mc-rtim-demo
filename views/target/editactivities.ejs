<script type="text/template" class="template" id="ELM.Activities.Templates.MetricsItem"><div class="well">
    <div class="row fixed">
        <div class="cell left middle">
            <h4>{{i18n "Name"}}</h4>
        </div>
        <div class="cell right middle">
            <button class="icon-delete deletemetric">{{i18n "Delete metric"}}</button>
        </div>
    </div>

    <div class="row fixed">
        <input type="text" name="metricname" maxlength="25" value="{{[jcr:title]}}" placeholder="{{i18n "Name of success metric"}}"/>
    </div>

    <div class="row fixed">
        <h4>{{i18n "What do you want to measure with this activity?"}}</h4>
    <span class="select select-metric-type" data-init="select">
      <button type="button" class="select-metric-type-btn"></button>
      <select>
          <option value="" {{#equals type ""}}selected="selected"{{/equals}}>{{i18n "Select success metric"}}</option>
          <option value="conversion" {{#equals type "conversion"}}selected="selected"{{/equals}}>{{i18n "Conversion"}}</option>
          <option value="revenue" {{#equals type "revenue"}}selected="selected"{{/equals}}>{{i18n "Revenue"}}</option>
          {{#if isGoalMetric}}
          {{#notEquals activityType '1-1'}}
          <option value="engagement" {{#equals type "engagement"}}selected="selected"{{/equals}}>{{i18n "Engagement"}}</option>
          {{/notEquals}}
          {{else}}
          <option value="engagement" {{#equals type "engagement"}}selected="selected"{{/equals}}>{{i18n "Engagement"}}</option>
          {{/if}}
      </select>
    </span>

        {{#unless type}}
        <div class="tooltip info arrow-left goalnote">{{i18n "This metric will determine the success of your activity"}}</div>
        {{/unless}}
    </div>

    {{#equals type "conversion"}}
    <div class="hidden">
      <span class="select select-counting-rule" data-init="select">
      <button type="button"></button>
        <select>
            {{!--
            <option value="oncePerVisitor" {{#equals countingRule "oncePerVisitor"}}selected="selected"{{/equals}}>{{i18n "Once per visitor"}}</option>
            --}}
            <option value="alwaysIncrement" selected="selected">{{i18n "Always increment"}}</option>
        </select>
      </span>
    </div>
    {{/equals}}

    {{#equals type "revenue"}}
    <div class="row fixed">
        <h4>{{i18n "Default View for Reporting"}}</h4>
      <span class="select select-revenue-type" data-init="select">
        <button type="button"></button>
        <select>
            <option value="rpv" {{#equals revenueType "rpv"}}selected="selected"{{/equals}}>{{i18n "Revenue Per Visitor (RPV)"}}</option>
            <option value="aov" {{#equals revenueType "aov"}}selected="selected"{{/equals}}>{{i18n "Average Order Value (AOV)"}}</option>
            {{#if isGoalMetric}}
            {{#notEquals activityType '1-1'}}
            {{#notEquals activityType 'MVT'}}
            <option value="sales" {{#equals revenueType "sales"}}selected="selected"{{/equals}}>{{i18n "Total Sales"}}</option>
            {{/notEquals}}
            <option value="conversionRate" {{#equals revenueType "conversionRate"}}selected="selected"{{/equals}}>{{i18n "Orders"}}</option>
            {{/notEquals}}
            {{else}}
            <option value="sales" {{#equals revenueType "sales"}}selected="selected"{{/equals}}>{{i18n "Total Sales"}}</option>
            <option value="conversionRate" {{#equals revenueType "conversionRate"}}selected="selected"{{/equals}}>{{i18n "Orders"}}</option>
            {{/if}}
        </select>
      </span>
    </div>
    {{/equals}}

    {{#equals type "engagement"}}
    <h4>{{i18n "There are three ways you can measure engagement, which would you like to measure?"}}</h4>
    <span class="select select-engagement-type" data-init="select">
      <button type="button"></button>
      <select>
          <option value="" {{#equals engagementType ""}}selected="selected"{{/equals}}>{{i18n "Select action"}}</option>
          <option value="pageViews" {{#equals engagementType "pageViews"}}selected="selected"{{/equals}}>{{i18n "Page Views"}}</option>
          <option value="customScoring" {{#equals engagementType "customScoring"}}selected="selected"{{/equals}}>{{i18n "Custom Scoring"}}</option>
          <option value="timeOnSite" {{#equals engagementType "timeOnSite"}}selected="selected"{{/equals}}>{{i18n "Time on Site"}}</option>
      </select>
    </span>

    {{#notIn activityType "Experience-Targeting" "MVT"}}
    {{#ifGlobalVar "ELM.isProjectedEarningsEnabled"}}
    {{#in engagementType "pageViews" "timeOnSite"}}
    <div class="row fixed metric-worth">
        <h4>
            {{#equals engagementType "timeOnSite"}}
            {{i18n "Estimated value of a minute on site"}}&nbsp;
            {{/equals}}
            {{#equals engagementType "pageViews"}}
            {{i18n "Estimated value of a page view"}}
            {{/equals}}
              <span class="morelink">
                &nbsp;<a>{{i18n "What is this"}}?</a>
                <span class="tooltip arrow-left">
                  {{i18n "Providing a value for your goal allows Target to calculate an estimated lift in revenue."}}&nbsp;
                  <a href="http://microsite.omniture.com/t2/help/en_US/target/#target-Creating_an_Activity" target="_blank">{{i18n "Learn more"}}</a>
                </span>
              </span>
        </h4>
        <input type="text" placeholder="{{getCurrencySymbol}}" value="{{#if worth}}{{number worth}}{{/if}}" name="metricWorth" class="metric-worth-input"/>
    </div>
    {{/in}}
    {{/ifGlobalVar}}
    {{/notIn}}
    {{/equals}}

    {{!--
    #Conversion Section
    --}}
    {{#if countingRule}}
    <h4>{{i18n "What action was taken by your audience to indicate your goal has been reached?"}}</h4>
    <div class="row fixed">
        <div class="cell actionsCell">
        <span class="select select-action" data-init="select">
          <button type="button">Select</button>
          <select>
              <option value="" {{#equals action ""}}selected="selected"{{/equals}}>{{i18n "Select action"}}</option>
              <option value="pageView" {{#equals action "pageView"}}selected="selected"{{/equals}}>{{i18n "Viewed a page"}}</option>
              <option value="mboxView" {{#equals action "mboxView"}}selected="selected"{{/equals}}>{{i18n "Viewed an mbox"}}</option>
              {{#equals activityType "recommendation"}}
              {{#ifGlobalVar "ELM.isRecsClickTrackEnabled"}}
              <option value="clickRecommendation" {{#equals action "clickRecommendation"}}selected="selected"{{/equals}}>{{i18n "Clicked on recommendation"}}</option>
              {{/ifGlobalVar}}
              {{else}}
              <option value="elementClick" {{#equals action "elementClick"}}selected="selected"{{/equals}}>{{i18n "Clicked an element"}}</option>
              {{/equals}}
          </select>
        </span>
        </div>

        {{#equals action "pageView"}}
        <div class="cell actionsCell">
          <span class="select select-matcher" data-init="select">
          <button type="button">Select</button>
            <select>
                <option value="exact" {{#equals matcher "exact"}}selected="selected"{{/equals}}>{{i18n "URL is"}}</option>
                <option value="contains" {{#equals matcher "contains"}}selected="selected"{{/equals}}>{{i18n "URL contains"}}</option>
            </select>
          </span>
        </div>

        <div class="cell">
            {{#each constraints}}
            {{> UrlField this}}
            {{/each}}
            <i class="icon-add-circle withLabel">{{i18n "Add URL"}}</i>
        </div>
        {{/equals}}

        {{#equals action "mboxView"}}
        <div class="cell constraintsCell">
            {{#each constraints}}
            <div class="constraint margin-t-05 mboxSearch">
                <div>
                    <span class="lh-2-375 margin-l-n-1-5 padding-r-0-5 {{#unless @index}}visibility-hidden{{/unless}}">{{i18n "OR"}}</span>
                    <select class="step mboxFilters mboxLookup{{@index}}" data-order="2" constraint-order="{{@index}}" data-init="filters" data-placeholder="{{i18n "Search mbox"}}" >
                    </select>
                    {{#unless ../specialmbox}}
                    {{#notEquals term ""}}
                    <i class="icon-target small withLabel additionalCondition {{#if [ref:targetingAudience]}}display-none{{/if}}" data-condition="{{@index}}">
                        {{i18n "Add audience targeting"}}
                    </i>
                    {{/notEquals}}
                    {{/unless}}
                    {{#if @index}}
                    <i class="icon-close-circle" id={{@index}}></i>
                    {{/if}}
                </div>
                <div class="targetingAudience{{@index}} margin-t-05 margin-l-0-5 {{#unless [ref:targetingAudience]}}display-none{{/unless}}">
                    {{#if [ref:targetingAudience]}}
                    <span class="font-size-0-8375">{{i18n "when visitors match"}}: <a class="icon link margin-l-0-2">{{audienceTitle}}</a></span>
                    {{else}}
                    <span class="font-size-0-8375">{{i18n "when visitors match"}}: <a class="icon link cursor-pointer addTargetingCondition" data-condition="{{@index}}">{{i18n "Add Audience/Targeting Condition"}}</a></span>
                    {{/if}}
                    <i class="icon-targeted small withLabel appliedCondition {{#unless [ref:targetingAudience]}}display-none{{/unless}}" data-condition="{{@index}}">{{i18n "Remove targeting"}}</i>
                </div>
            </div>
            {{/each}}
            {{#unless specialmbox}}
            <i class="icon-add-circle withLabel">{{i18n "Add an Mbox"}}</i>
            {{/unless}}
        </div>
        {{/equals}}

        {{#equals action "elementClick"}}
        <div class="cell constraintsCell">
            {{#if constraints.length}}
            <span>&nbsp;{{constraints.length}}&nbsp;{{i18n "selected"}}&nbsp;</span>
