        {{#with targets.template.targets.[0]}}
        <span>{{i18n "<strong>100% of the activity entrants</strong> to any page where" html=true}}&nbsp;</span>
        <span class="testing-template-rules cursor-pointer link">
          {{#if children}}
            {{#with children.[0]}}
              <strong>{{getTemplateTestingCriteriaParameter parameter type subParameter}}</strong>&nbsp;{{getTemplateTestingCriteriaOperator operator}}&nbsp;<strong>{{join operands ", "}}</strong>
            {{/with}}
            {{#gt children.length 1}}
              &nbsp;<strong>(+{{subtract children.length 1}}&nbsp;{{i18n "more"}})</strong>
            {{/gt}}
          {{else}}
            <strong>{{getTemplateTestingCriteriaParameter parameter type subParameter}}</strong>&nbsp;{{getTemplateTestingCriteriaOperator operator}}&nbsp;<strong>{{join operands ", "}}</strong>
          {{/if}}
        </span>
        <span>
          &nbsp;{{i18n "will be included."}}
