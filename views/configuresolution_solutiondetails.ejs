<div class="solution-container">
    <label class="description-label">Solution</label>
    <div class="solution-selection border-container">
        <div class="border-content">
            <label class="label-description">Select the solution you would like to have this data sent to and made available.</label>
            <div class="coral-Autocomplete" data-init="autocomplete">
                <div class="coral-InputGroup coral-InputGroup--block js-coral-Autocomplete-field">
                    <label class="coral-DecoratedTextfield coral-InputGroup-input">
                        <span class="u-coral-screenReaderOnly">Select a solution</span>
                        <span class="coral-DecoratedTextfield-icon coral-Icon coral-Icon--sizeXS coral-Icon--search" aria-hidden="true"></span>
                        <input class="coral-DecoratedTextfield-input coral-Textfield js-coral-Autocomplete-textfield" type="text" name="name1" placeholder="Select a solution" >
                    </label>
                    <span class="coral-InputGroup-button">
                        <button class="coral-Button coral-Button--secondary coral-Button--square js-coral-Autocomplete-toggleButton" type="button">
                            <i class="coral-Icon coral-Icon--sizeXS coral-Icon--triangleDown"></i>
                        </button>
                    </span>
                </div>
                <ul class="coral-SelectList js-coral-Autocomplete-selectList">
                    <li class="coral-SelectList-item coral-SelectList-item--option" data-value="Analytics" data-path="/crs/datasources.reportsuites.html">Analytics</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="select-solution-container">
        
    </div>
    <label class="description-label">Attributes</label>
    <div class="attribute-container border-container" data-analytics-premium-url="https://sc.omniture.com/p/suite/1.3/index.html?a=Landing.Main&IMS=1">
        <div class="border-content">
            
            <div class="attribute-list">
                <button type="button" class="coral-Select-button coral-MinimalButton" data-toggle="selectlist" data-target="#attribute-selection">
                    <span class="coral-Select-button-text">Add Attribute</span>
                </button>

                <ul id="attribute-selection" class="coral-SelectList" data-init="selectlist" data-multiple="true">
                    
                    <li class="coral-SelectList-item coral-SelectList-item--option coral-Checkbox" data-value="-1" data-display-name="Select All" data-description="Select All" data-isChecked="none">
                        <input class="coral-Checkbox-input select-all" type="checkbox"/>
                        <span class="coral-Checkbox-checkmark"></span>
                        <span class="coral-Checkbox-description">Select All</span>
                    </li>

                    <% attributes.forEach(function(attribute) { %>
                        <li class="coral-SelectList-item coral-SelectList-item--option coral-Checkbox options" data-value="<%= attribute.name %>" data-display-name="<%= attribute.displayName %>" data-description="<%= attribute.description %>" data-should-warn="false" data-is-new-attribute="false">
                            <input class="coral-Checkbox-input" type="checkbox"/>
                            <span class="coral-Checkbox-checkmark"></span>
                            <span class="coral-Checkbox-description"><%= attribute.description %></span>
                        </li>
                    <% }); %>

                </ul>
                
            </div>
            <div class="attribute-table hidden" >
                <ul class="list">
                    <li class="header">
                        <div class="row fixed listHeader">
                            <div class="row fixed listHeader">
                                <div class="cell middle delete-row"></div>
                                <div class="cell middle">Attribute</div>
                                <div class="cell middle">Display Name</div>
                                <div class="cell middle">Description</div>
                            </div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
